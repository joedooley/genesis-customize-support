{
	"name": "devdesigns/genesis-customizer-support",
	"type": "wordpress-plugin",
	"description": "Adds customizer support to your Genesis child theme.",
	"homepage": "https://github.com/joedooley/genesis-customizer-support",
	"keywords": [ "wordpress", "wordpress-plugin", "genesis", "genesis-framework", "customizer" ],
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Joe Dooley",
			"email": "hello@developingdesigns.com",
			"homepage": "https://twitter.com/devdesignstampa/",
			"role": "Developer"
		},
		{
			"name": "Developing Designs",
			"email": "hello@developingdesigns.com",
			"homepage": "https://developingdesigns.com/",
			"role": "Company"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"require": {
		"php": "^7.2",
		"composer/installers": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"DevDesigns\\GenesisCustomizerSupport\\": "src/"
		},
		"files": ["src/functions.php"]
	}
}
